import tkinter  as Tk
from PIL import Image,ImageTk
# public function cadastraconta(kgasto,dtleitura)
janela = Tk.Tk()
janela.state('zoomed')
janela.title("Teste Principal")

janela.grid_columnconfigure(1,minsize=2)
janela.grid_rowconfigure(0,minsize=100)
titulo = Tk.Label(text="LOJA DE CONVÊNIENCIA NAUM",font=100,fg="white",bg="#000000",width=180,height=5)
titulo.grid(row=0,column=0,columnspan=4)
#COMIDA
def comida():
    comidas = Tk.Toplevel()
    comidas.state('zoomed')
    comidas.title("Comida")
 
    titulo = Tk.Label (comidas,text="COMIDAS",fg="black",bg="#FFFF00",width=230,height=5).grid(row=0,column=0,columnspan=4)
    tkimagemc0 = ImageTk.PhotoImage(Image.open("imagem/pizza.jpg"))
    Tk.Label(comidas,image=tkimagemc0).grid(row=1,column=0)
    #
    tkimagemc1 = ImageTk.PhotoImage(Image.open("imagem/sorvete.jpg"))
    Tk.Label(comidas,image=tkimagemc1).grid(row=1,column=1)
    #
    tkimagemc2 = ImageTk.PhotoImage(Image.open("imagem/coxinha.jpg"))
    Tk.Label(comidas,image=tkimagemc2).grid(row=1,column=2)
    #
    tkimagemc3 = ImageTk.PhotoImage(Image.open("imagem/sanduiche.jpg"))
    Tk.Label(comidas,image=tkimagemc3).grid(row=1,column=3)
    #
    tkimagemc4 = ImageTk.PhotoImage(Image.open("imagem/bolo.jpg"))
    Tk.Label(comidas,image=tkimagemc4).grid(row=5,column=0)
    #
    tkimagemc5 = ImageTk.PhotoImage(Image.open("imagem/chips.jpg"))
    Tk.Label(comidas,image=tkimagemc5).grid(row=5,column=1)
    #
    tkimagemc6 = ImageTk.PhotoImage(Image.open("imagem/paoque.jpg"))
    Tk.Label(comidas,image=tkimagemc6).grid(row=5,column=2)
    #
    tkimagemc7 = ImageTk.PhotoImage(Image.open("imagem/pingo.jpg"))
    Tk.Label(comidas,image=tkimagemc7).grid(row=5,column=3)
    botao_fechar =Tk.Button(comidas,text="Voltar",command=comidas.destroy).grid(row=0,column=0)
    comidas.mainloop()
seila = []
print(seila)

def objeto():
    obj = Tk.Toplevel()
    obj.state('zoomed')
    obj.title("Comida")
    titulo = Tk.Label (obj,text="OBJETO",fg="white",bg="#990000",width=230,height=5).grid(row=0,column=0,columnspan=4)
    tkimagemob0 = ImageTk.PhotoImage(Image.open("imagem/bolinha.jpg"))
    Tk.Label(obj,image=tkimagemob0).grid(row=1,column=0)
    nomeob0 =Tk.Label(obj,text="Bolinha Pula-Pula(1 unidade)")
    nomeob0.grid(row=2,column=0)
    seila.append(nomeob0)
    precoob0 = Tk.Label(obj,text="R$ 2,00")
    precoob0.grid(row=3,column=0)
    compraob0 =Tk.Button(obj,text="Comprar")
    compraob0.grid(row=4,column=0)
    #
    tkimagemob1 = ImageTk.PhotoImage(Image.open("imagem/dino.jpg"))
    Tk.Label(obj,image=tkimagemob1).grid(row=1,column=1)
    #
    tkimagemob2 = ImageTk.PhotoImage(Image.open("imagem/chaveiro.jpg"))
    Tk.Label(obj,image=tkimagemob2).grid(row=1,column=2)
    #
    tkimagemob3 = ImageTk.PhotoImage(Image.open("imagem/oculos.jpg"))
    Tk.Label(obj,image=tkimagemob3).grid(row=1,column=3)
    #
    tkimagemob4 = ImageTk.PhotoImage(Image.open("imagem/cd.jpg"))
    Tk.Label(obj,image=tkimagemob4).grid(row=5,column=0)
    #
    tkimagemob5 = ImageTk.PhotoImage(Image.open("imagem/xicara.jpg"))
    Tk.Label(obj,image=tkimagemob5).grid(row=5,column=1)
    #
    tkimagemob6 = ImageTk.PhotoImage(Image.open("imagem/Boneca.png"))
    Tk.Label(obj,image=tkimagemob6).grid(row=5,column=2)
    #
    tkimagemob7= ImageTk.PhotoImage(Image.open("imagem/cubo.jpg"))
    Tk.Label(obj,image=tkimagemob7).grid(row=5,column=3)
    botao_fechar =Tk.Button(obj,text="Voltar",command=obj.destroy,height=2).grid(row=0,column=0)
    obj.mainloop()
