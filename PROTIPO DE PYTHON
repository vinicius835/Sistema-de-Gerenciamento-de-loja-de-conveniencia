def login():
    log = Tk.Toplevel()
    log.title('Login')

    # log.state("zoomed")

    log.rowconfigure(0,weight=1)
    log.columnconfigure(0,weight=1)
    log.resizable(False,False)

    #Cores Fundo
    # Row 00
    Cor_01 = Tk.Label(log,text='LOGIN',fg='Black', bg='#C0C0C0',width=40,height=3,font=10)
    Cor_01.grid(row=0,column=1,sticky='NSEW',columnspan=4)
    # Row 06
    Cor_02 = Tk.Label(log,fg='Black', bg='#C0C0C0',width=40,height=1)
    Cor_02.grid(row=8,column=1,sticky='NSEW',columnspan=4)

    #Espaço em Branco
    # Row 01
    CorB_01 = Tk.Label(log)
    CorB_01.grid(row=1,column=1)
    # Row 02
    CorB_02 = Tk.Label(log)
    CorB_02.grid(row=3,column=1)

    # Row 05
    CorB_02 = Tk.Label(log)
    CorB_02.grid(row=5,column=1)

    # Row 07
    CorB_03 = Tk.Label(log)
    CorB_03.grid(row=7,column=1)

    #Entrada Usuário
    TextoUsuario = Tk.Label(log,text='Usuário:')
    TextoUsuario.grid(row=2,column=1)

    Usuario = Tk.Text(log,width=35,height=1)
    Usuario.grid(row=2,column=2)

    #Entrada Senha
    TextoSenha = Tk.Label(log,text='Senha:')
    TextoSenha.grid(row=4,column=1)

    Senha = Tk.Text(log,width=35,height=1)
    Senha.grid(row=4,column=2)

    #Botão Confirmação
    BotaoLogin = Tk.Button(log,text='Confirmar Login')
    BotaoLogin.grid(row=6,columnspan=3)

    # Informações Login (Usuário e Senha)
    def InfoLogin():
        print()

    log.mainloop()

def carrinho():
    arroz = 1
botao_comida =Tk.Button(janela,text="COMIDA",width=10,height=2,border=3,command=comida).grid(row=1,column=0)
botao_obj =Tk.Button(janela,text="OBJETO",width=10,height=2,border=3,command=objeto).grid(row=1,column=1)
botao_bb =Tk.Button(janela,text="BEBIDA",width=10,height=2,border=3,command=bebida).grid(row=1,column=2)
botao_limpz =Tk.Button(janela,text="LIMPEZA/HIGIENE",width=15,height=2,border=3,command=limpeza).grid(row=1,column=3)
botao_login =Tk.Button(janela,text="LOGIN",width=10,height=2,border=3,command=login).grid(row=0,column=3)
botao_carro =Tk.Button(janela,text="CARRINHO",width=15,height=2,border=3,command=carrinho)
botao_carro.grid(row=0,column=2)

janela.mainloop()


